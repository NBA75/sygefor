{
    "name": "conjecto/sygefor3",
    "license": "MIT",
    "type": "project",
    "description": "Sygefor 3 project",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rmoreas/ShibbolethBundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "swiftmailer/swiftmailer": "~5.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "ruflin/elastica": "~1.3.0",
        "friendsofsymfony/user-bundle": "~1.3",
        "friendsofsymfony/oauth-server-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "friendsofsymfony/elastica-bundle": "~3.0",
        "mopa/bootstrap-bundle": "dev-master",
        "knplabs/knp-menu":"2.0.*@dev",
        "knplabs/knp-menu-bundle":"2.0.*@dev",
        "knplabs/doctrine-behaviors": "1.0.*@dev",
        "jms/security-extra-bundle": "dev-master",
        "jms/serializer-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "friendsofsymfony/rest-bundle": "1.3.*",
        "volcanus/csv": "dev-master",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "mbence/opentbs-bundle": "dev-master",
        "knplabs/knp-snappy-bundle": "dev-master",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "nelmio/cors-bundle": "~1.0",
        "liuggio/ExcelBundle": "~2.0",
        "kuleuven/shibboleth-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "fzaninotto/faker": "1.3.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
